{
  "name": "@skax/pnpm-workspace",
  "version": "0.0.0",
  "description": "pnpm workspace",
  "scripts": {
    "dev": "pnpm run --filter '*' dev",
    "dev:pkg": "pnpm run --filter @ak2021/pkg dev",
    "dev:store": "pnpm run --filter @ak2021/store dev",
    "dev:ui": "pnpm run --filter @ak2021/react-ui dev",
    "dev:components": "pnpm run --filter @ak2021/web-components dev",
    "dev:webpack": "pnpm run --filter @ak2021/with-webpack dev",
    "dev:react": "pnpm run --filter @site/with-react dev",
    "dev:vue2": "pnpm run --filter @site/with-vue2 dev",
    "dev:vue3": "pnpm run --filter @site/with-vue3 dev",
    "build": "pnpm run --filter '*' build",
    "build:pkg": "pnpm run --filter @ak2021/pkg build",
    "build:store": "pnpm run --filter @ak2021/store build",
    "build:ui": "pnpm run --filter @ak2021/react-ui build",
    "build:components": "pnpm run --filter @ak2021/web-components build",
    "build:webpack": "pnpm run --filter @ak2021/with-webpack build",
    "build:react": "pnpm run --filter @site/with-react build",
    "build:vue2": "pnpm run --filter @site/with-vue2 build",
    "build:vue3": "pnpm run --filter @site/with-vue3 build",
    "lint": "eslint --fix \"packages/*/src/*.{ts,tsx,js,jsx,mjs}\" \"common/*/src/*.{ts,tsx,js,jsx,mjs}\" \"site/*/src/*.{ts,tsx,js,jsx,mjs}\" \"config/*/src/*.{js,mjs}\"",
    "fmt": "prettier --write \"./**/*.{ts,tsx,js,jsx,mjs}\"",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "changeset publish",
    "list": "lerna list",
    "release": "lerna publish"
  },
  "files": [],
  "keywords": [],
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "changeset": "^0.2.6",
    "cross-env": "^10.1.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.39.2",
    "eslint-config-xx": "^2.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.1.2",
    "rollup": "^4.54.0",
    "typescript": "5.3.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "lerna": "8.2.4"
  },
  "engines": {
    "node": ">=18.3",
    "react": ">=17"
  },
  "publishConfig": {
    "access": "public"
  }
}
