{
  "name": "pnpm-workspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "site": "npm run dev",
    "dev": "pnpm run --filter site-dumi dev",
    "dev:react": "pnpm run --filter with-react start",
    "dev:vue3": "pnpm run --filter with-vue3 dev",
    "dev:vue2": "pnpm run --filter with-vue2 serve",
    "build:pkg1": "pnpm run --filter pkg1 build",
    "build:pkg2": "pnpm run --filter pkg1 build",
    "build:utils": "pnpm run --filter @common/utils build",
    "build": "pnpm run --filter '*' build",
    "build:site": "pnpm run --filter site-dumi build",
    "build:site:react": "pnpm run --filter with-react build",
    "build:site:vue3": "pnpm run --filter with-vue3 build",
    "build:site:vue2": "pnpm run --filter with-vue2 build",
    "lint": "eslint --fix \"packages/*/src/*.{ts,tsx,js,jsx}\" \"common/*/src/*.{ts,tsx,js,jsx}\" \"site/*/src/*.{ts,tsx,js,jsx}\"",
    "fmt": "prettier --write \"./**/*.{ts,tsx,js,jsx}\"",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "changeset": "^0.2.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "eslint": "^8.57.0",
    "eslint-config-xx": "^1.10.0",
    "husky": "^9.1.3",
    "lodash-es": "^4.17.21",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.0",
    "typescript": "5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "@ak2021/rollup": "workspace:*"
  },
  "engines": {
    "node": ">=18.3"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
