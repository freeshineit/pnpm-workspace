{
  "name": "pnpm-workspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:pkg1": "pnpm run --filter pkg1 dev",
    "dev:pkg2": "pnpm run --filter pkg1 dev",
    "dev:utils": "pnpm run --filter @common/utils dev",
    "dev:all": "pnpm run --filter '*' dev",
    "dev": "pnpm run --filter site dev",
    "site": "pnpm run --filter site dev",
    "build:pkg1": "pnpm run --filter pkg1 build",
    "build:pkg2": "pnpm run --filter pkg1 build",
    "build:utils": "pnpm run --filter @common/utils build",
    "build": "pnpm run --filter '*' build",
    "build:site": "pnpm run --filter site build",
    "lint": "eslint packages/*/src/* common/*/src/* site/src/* --fix --ext .ts,.tsx,.js,.jsx",
    "fmt": "prettier --write \"./**/*.{ts,tsx,js,jsx}\"",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-swc": "^0.3.0",
    "@swc/core": "^1.4.11",
    "@types/lodash-es": "^4.17.12",
    "changeset": "^0.2.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-xx": "^1.10.0",
    "husky": "^9.0.11",
    "lodash-es": "^4.17.21",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.0",
    "rollup-plugin-serve": "^2.0.3",
    "typescript": "5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18"
  }
}
