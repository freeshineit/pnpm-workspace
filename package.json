{
  "name": "@skax/pnpm-workspace",
  "version": "0.1.0",
  "description": "pnpm workspace",
  "scripts": {
    "dev:common": "pnpm run --filter '@common/*' dev",
    "dev:pkg2": "pnpm run --filter '@package/pkg2' dev",
    "dev:store": "pnpm run --filter '@package/store' dev",
    "dev:ui": "pnpm run --filter '@package/ui' dev",
    "dev:webcomponents": "pnpm run --filter '@package/web-components' dev",
    "dev:webpack": "pnpm run --filter '@package/with-webpack' dev",
    "build": "pnpm run --filter '*' build",
    "build:common": "pnpm run --filter '@common/*' build",
    "build:pkg2": "pnpm run --filter '@package/pkg2' build",
    "build:store": "pnpm run --filter '@package/store' build",
    "build:ui": "pnpm run --filter '@package/ui' build",
    "build:webcomponents": "pnpm run --filter '@package/web-components' build",
    "build:webpack": "pnpm run --filter '@package/with-webpack' build",
    "build:site": "pnpm run --filter site-dumi build",
    "build:site:react": "pnpm run --filter with-react build",
    "build:site:vue3": "pnpm run --filter with-vue3 build",
    "build:site:vue2": "pnpm run --filter with-vue2 build",
    "lint": "eslint --fix \"packages/*/src/*.{ts,tsx,js,jsx,mjs}\" \"common/*/src/*.{ts,tsx,js,jsx,mjs}\" \"site/*/src/*.{ts,tsx,js,jsx,mjs}\" \"config/*/src/*.{js,mjs}\"",
    "fmt": "prettier --write \"./**/*.{ts,tsx,js,jsx}\"",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "files": [],
  "keywords": [],
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "changeset": "^0.2.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.0",
    "eslint-config-xx": "^1.10.1",
    "husky": "^9.1.6",
    "lodash-es": "^4.17.21",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.24.0",
    "typescript": "5.3.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "engines": {
    "node": ">=18.3",
    "react": ">=17"
  },
  "publishConfig": {
    "access": "public"
  }
}
